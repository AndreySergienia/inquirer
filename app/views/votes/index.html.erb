<div class="btn-group">
  <%= link_to "Create new vote", new_vote_path, class: "btn btn-success" %>
</div>
<p></p>
<% @votes.each do |vote| %>
  <div class="well">
    <h4 class="col-md-4"><%= link_to "#{vote.name}", vote_path(vote) %></h4>
    <div class="text-right">
      <%= link_to "Edit", edit_vote_path(vote), class: "btn btn-default" %>
      <%= link_to "Delete", vote_path(vote), method: :delete, class: "btn btn-danger", data: {confirm: "Are you sure?"} %>
    </div>
  </div>
<% end %>
